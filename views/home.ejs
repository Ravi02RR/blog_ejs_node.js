<%- include("partials/header");-%>
    <h1>Home!</h1>
    <p>
        <%=homeStartingContent %>
    </p>

    <% for(var i=0; i<posts.length; i++){ %>
        <div class="item">
            <h1>

                <%=posts[i].title%>
            </h1>
            <p>
                <%=posts[i].content.substring(0, 100) + "..." %>

            </p>
            <a href="/posts/ <%=posts[i].title%>">Read More</a>

        </div>
        <% } %>
            <%- include("partials/footer");-%>